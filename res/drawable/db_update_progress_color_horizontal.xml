<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="20.0dip" />
            <gradient android:startColor="#ffababab" android:endColor="#ffababab" android:angle="270.0" android:centerY="0.75" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="20.0dip" />
                <gradient android:startColor="#ff0c5ee5" android:endColor="#ff0c5ee5" android:angle="270.0" android:centerY="0.75" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="20.0dip" />
                <gradient android:startColor="#fffecd3d" android:endColor="#fffecd3d" android:angle="270.0" android:centerY="0.75" />
            </shape>
        </clip>
    </item>
</layer-list>
